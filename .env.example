# Copy the relevant variables into .env.<dialect> files to customize credentials locally.

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=customer_profile
POSTGRES_PORT=5432

# MySQL / MariaDB
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=customer_profile
MYSQL_USER=dev
MYSQL_PASSWORD=devpass
MYSQL_PORT=3306
MARIADB_ROOT_PASSWORD=root
MARIADB_DATABASE=customer_profile
MARIADB_USER=dev
MARIADB_PASSWORD=devpass
MARIADB_PORT=3307

# SQL Server
MSSQL_SA_PASSWORD=LocalStrongPassw0rd!
MSSQL_PORT=1433

# SQLite / DuckDB
SQLITE_DB_PATH=./db/sqlite/customer_profile.db
DUCKDB_PATH=./db/duckdb/customer_profile.duckdb

# Oracle
ORACLE_PWD=Local0raclePwd!
ORACLE_PORT=1521

# Snowflake
SNOWFLAKE_ACCOUNT=example-account
SNOWFLAKE_USER=developer
SNOWFLAKE_ROLE=SYSADMIN
SNOWFLAKE_WAREHOUSE=DEV_WH
SNOWFLAKE_DATABASE=CUSTOMER_PROFILE

# BigQuery Emulator
BIGQUERY_PROJECT=customer-profile-local
BIGQUERY_DATASET=customer_profile
BIGQUERY_PORT=9050
